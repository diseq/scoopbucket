{
    "version": "5.14.0",
    "description": "A cross-platform encrypted messaging service.",
    "homepage": "https://signal.org",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://updates.signal.org/desktop/signal-desktop-win-5.14.0.exe#/dl.7z",
            "hash": "sha512:52bd20638c437e500a8cc0e6b075567653f7e680903746c40797ad7258b68d12937cfaa130d47b9661fccacdc7c494cd0d25ad21f215a1d783fd8f6e8964558c",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninstall*.exe\" -Recurse -Force",
                "Remove-Item \"$dir\\resources\\app-update.yml\" -Recurse -Force"
            ]
        }
    },
    "bin": "signal.exe",
    "shortcuts": [
        [
            "signal.exe",
            "Signal"
        ]
    ],
    "checkver": {
        "url": "https://updates.signal.org/desktop/latest.yml",
        "regex": "version:\\s([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://updates.signal.org/desktop/signal-desktop-win-$version.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512:\\s$base64"
        }
    }
}
